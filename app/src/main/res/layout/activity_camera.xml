<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             xmlns:kimdata="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.main.harriscam.CameraActivity">

    <com.view.harriscam.CameraSurfaceView
        android:id="@+id/cameraSurfaceView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.view.harriscam.SlideMenuView
        android:id="@+id/slideMenu"
        kimdata:isVisibleMode="false"
        kimdata:isVisibleOptions="false"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</FrameLayout>
